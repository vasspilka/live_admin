<main class="main__content">
  <div class="main__wrap">
    <.live_component
      id="nav"
      module={@nav_mod}
      title={@title}
      base_path={@base_path}
      resources={@resources}
      resource={assigns[:resource]}
      prefix={assigns[:prefix]}
      key={assigns[:key]}
    />
    <div class="content">
      <%= @inner_content %>
    </div>
  </div>
</main>
